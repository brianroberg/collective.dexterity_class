<?xml version="1.0" ?>
<model xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="text_line" type="zope.schema.TextLine">
      <description>help for text line</description>
      <title>Text Line</title>
      <required>False</required>
    </field>
    <field name="boolean_field" type="zope.schema.Bool">
      <description />
      <required>False</required>
      <title>Boolean Field</title>
    </field>
    <field name="choice_field" type="zope.schema.Choice">
      <default>One</default>
      <description />
      <missing_value>One</missing_value>
      <required>False</required>
      <title>Choice Field</title>
      <values>
        <element>One</element>
        <element>Two</element>
        <element>Three</element>
      </values>
    </field>
    <field name="date_time_field" type="zope.schema.Datetime">
      <description />
      <required>False</required>
      <title>Date-Time Field</title>
    </field>
    <field name="file_upload" type="plone.namedfile.field.NamedFile">
      <description />
      <required>False</required>
      <title>File Upload</title>
    </field>
    <field name="floater" type="zope.schema.Float">
      <description />
      <required>False</required>
      <title>Floater</title>
    </field>
    <field name="image_field" type="plone.namedfile.field.NamedImage">
      <description />
      <required>False</required>
      <title>Image Field</title>
    </field>
    <field name="integer_field" type="zope.schema.Int">
      <description />
      <required>False</required>
      <title>Integer Field</title>
    </field>
    <field name="multiple_choice_field" type="zope.schema.List">
      <description />
      <required>False</required>
      <title>Multiple Choice Field</title>
      <value_type type="zope.schema.Choice">
        <values>
          <element>One</element>
          <element>Two</element>
          <element>Three</element>
        </values>
      </value_type>
    </field>
    <field name="rich_text_field" type="plone.app.textfield.RichText">
      <description />
      <required>False</required>
      <title>Rich Text Field</title>
    </field>
    <field name="text_field" type="zope.schema.Text">
      <description />
      <required>False</required>
      <title>Text Field</title>
    </field>
  </schema>
</model>